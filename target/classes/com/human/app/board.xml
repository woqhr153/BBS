<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.human.app.Board">
	<select id="getBoardList" resultType="com.human.app.Boardinfo">
		select * from bbs
	</select>
	<select id="getBoardView" resultType="com.human.app.Boardinfo">
		select * from bbs where bbs_id = #{param1}
	</select>
</mapper>